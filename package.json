{
  "name": "specman-web-app",
  "version": "1.0.0",
  "description": "Specification manager for Regal American Homes",
  "main": "index.js",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "start": "node ./server/server.js",
    "build:css": "compass compile -c config/compass.rb",
    "build:js": "webpack --config ./webpack/prod.config.js",
    "build": "npm run build:js && npm run build:css",
    "postinstall": "npm run build",
    "watch:js": "webpack --config ./webpack/dev.config.js",
    "watch:css": "compass watch -c config/compass.rb",
    "watch": "npm run watch:js & npm run watch:css",
    "lint": "eslint ./src",
    "webpack": "webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "sourceType": "module"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:vdz/specmanapp.git"
  },
  "keywords": [
    ""
  ],
  "author": "Yehuda Gilead yehuda@chegg.com",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.5",
    "epilogue": "^0.7.1",
    "express": "^4.14.0",
    "fs-promise": "^0.5.0",
    "json-loader": "^0.5.4",
    "mysql": "^2.12.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.2.0",
    "react-edit-inline": "https://github.com/vdz/ReactInlineEdit.git",
    "react-dom": "^15.2.0",
    "react-dropzone": "^3.5.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.76.0",
    "sequelize": "^3.28.0",
    "url-pattern": "^1.0.3",
    "webpack": "^1.13.1",
    "yamlparser": "0.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack2": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0"
  }
}
